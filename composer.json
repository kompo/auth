{
    "name": "kompo/auth",
    "description": "Authentication and teams to your kompo application",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kompo\\Auth\\": "src/",
            "Kompo\\Auth\\Database\\Factories\\": "database/factories/",
            "Kompo\\Auth\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kompo\\Auth\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=5.6.0",
        "kompo/kompo": "^3.0",
        "laravel/fortify": "^1.19",
        "lab404/laravel-impersonate": "^1.7",
        "arcanedev/log-viewer": "^10.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-mail-preview": "^6.0",
        "laravel/socialite": "^5.12",
        "socialiteproviders/microsoft-azure": "^5.2",
        "maatwebsite/excel": "^3.1",
        "laravel/vonage-notification-channel": "^3.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "condoedge/utils": "^v0.2.12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.0|^9.0",
        "brianium/paratest": "^7.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kompo\\Auth\\KompoAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "test": [
            "@php vendor/bin/testbench migrate",
            "@php vendor/bin/testbench schema:dump",
            "@php vendor/bin/testbench package:test"
        ],
        "test-coverage": [
            "@php vendor/bin/testbench migrate",
            "@php vendor/bin/testbench schema:dump",
            "@php vendor/bin/testbench package:test --coverage-html coverage-html"
        ],
        "test-testdox": [
            "@php vendor/bin/testbench migrate",
            "@php vendor/bin/testbench schema:dump",
            "@php vendor/bin/testbench package:test --testdox"
        ]
    }
}
