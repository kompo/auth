{
    "name": "kompo/auth",
    "description": "Authentication and teams to your kompo application",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Kompo\\Auth\\": "src/",
            "Kompo\\Auth\\Database\\Factories\\": "database/factories/"
        }
    },
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": ">=5.6.0",
        "kompo/kompo": "^3.0",
        "laravel/fortify": "^1.19",
        "lab404/laravel-impersonate": "^1.7",
        "arcanedev/log-viewer": "^10.0",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-mail-preview": "^6.0",
        "laravel/socialite": "^5.12",
        "socialiteproviders/microsoft-azure": "^5.2",
        "maatwebsite/excel": "^3.1",
        "laravel/vonage-notification-channel": "^3.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "condoedge/utils": "^v0.2.12"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kompo\\Auth\\KompoAuthServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
